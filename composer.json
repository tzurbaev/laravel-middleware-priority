{
    "name": "laniakea/middleware-priority",
    "description": "More flexible way to manage Laravel's HTTP middleware priority.",
    "keywords": ["laravel", "middleware"],
    "homepage": "https://github.com/tzurbaev/laravel-middleware-priority",
    "license": "MIT",
    "authors": [
        {
            "name": "Timur Zurbaev",
            "email": "zurbaev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.21.1",
        "orchestra/testbench": "^9.0.0",
        "pestphp/pest": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Laniakea\\MiddlewarePriority\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laniakea\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "fix": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
